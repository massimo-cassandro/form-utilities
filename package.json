{
  "name": "@massimo-cassandro/form-utilities",
  "description": "Form utilities",
  "version": "1.1.1",
  "license": "MIT",
  "author": "Massimo Cassandro",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/massimo-cassandro/form-utilities.git#main"
  },
  "bugs": {
    "url": "https://github.com/form-utilities/issues"
  },
  "keywords": [],
  "scripts": {
    "UPD-version": "npx update-version --config=./dev-utilities.config.mjs",
    "upd@m": "npx upd@m",
    "npm publish": "npm publish"
  },
  "browserslist": [
    "last 1 version",
    "iOS >= 13",
    "Safari >= 12",
    "not ie <= 11",
    "> 3% in IT"
  ],
  "stylelint": {
    "extends": "@massimo-cassandro/linters-config/stylelintrc.js",
    "ignoreFiles": [
      "**/*.css"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "./node_modules/@massimo-cassandro/linters-config/eslintrc.js"
    ],
    "ignorePatterns": []
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "files": [
    "src/**/*.{scss,css,js,map}",
    "scss/**/*.scss",
    "index.js"
  ],
  "devDependencies": {
    "@massimo-cassandro/dev-utilities": "^1.12.5",
    "@massimo-cassandro/linters-config": "^1.3.9"
  }
}